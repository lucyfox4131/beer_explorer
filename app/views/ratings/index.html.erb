<div class="container">
<h1 class="content-header">All Rated Beers</h1>
<div class="col-md-6">
  <h3 class="content-header">All Liked Beers</h3>
  <div class="liked">
    <ul class="list-group">
      <% @liked.each do |beer| %>
      <li class="list-group-item beer-name-list"> <%= link_to beer.name, beer_path(beer.api_id),  class: "header-links" %> </li>
      <% end %>
    </ul>
  </div>
</div>

<div class="col-md-6">
  <h3 class="content-header">All Disliked Beers</h3>
  <div class="disliked">
    <ul class="list-group">
      <% @disliked.each do |beer| %>
      <li class="list-group-item beer-name-list"> <%= link_to beer.name, beer_path(beer.api_id),  class: "header-links" %> </li>
      <% end %>
    </ul>
  </div>
</div>
</div>
